    <div class="search-results">
      <% if @results.present? %>
        <h2>Search Results</h2>
        <ul>
          <% @results.each do |result| %>

              <li><%= link_to result.skaterFullName, result %></li>

          <% end %>
        </ul>
      <% else %>
        <p>No results found.</p>
      <% end %>
    </div>